      "source": [
        "n = 1042000\n",
        "while n <= 702648265:\n",
        "    order = len(str(n))\n",
        "    s=0\n",
        "    num = n\n",
        "    \n",
        "    while num > 0:  \n",
        "        digit = num % 10\n",
        "        s += digit ** order\n",
        "        num //= 10\n",
        "    if n == s:\n",
        "        print(\"The first Armstrong number is :\",n)\n",
        "        break  \n",
        "    n += 1"
      ],
